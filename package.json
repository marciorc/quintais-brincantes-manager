{
  "name": "quintais-brincantes-manager",
  "version": "1.0.0",
  "description": "Aplicação Web e de gerenciamento de Quintais Brincantes",
  "scripts": {
    "dev:api": "nodemon apps/api/server.js",
    "dev:web": "cd apps/web && npm run dev",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "prisma:migrate": "prisma migrate dev --schema=prisma/schema.prisma",
    "prisma:generate": "prisma generate",
    "prisma:seed": "node prisma/seed-superuser.js",
    "start": "node apps/api/server.js",
    "test": "cross-env NODE_ENV=test mocha apps/api/test/**/*.test.js --timeout 10000 --exit",
    "test:watch": "cross-env NODE_ENV=test mocha apps/api/test/**/*.test.js --watch --timeout 10000",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha apps/api/test/**/*.test.js --timeout 10000 --exit",
    "test:load:smoke": "cd apps/api/k6-tests && npm run test:smoke",
    "test:load:load": "cd apps/api/k6-tests && npm run test:load",
    "test:load:stress": "cd apps/api/k6-tests && npm run test:stress",
    "test:load:all": "cd apps/api/k6-tests && ./run-tests.sh all"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "cypress": "^14.5.4",
    "nodemon": "^3.1.10",
    "prisma": "^6.13.0"
  }
}
